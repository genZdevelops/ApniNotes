{
  "name": "Flutter Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/ApniNotes/frontend",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/ApniNotes,type=bind,consistency=cached",
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.flutter",
        "dart-code.dart-code"
      ]
    }
  },
  "postCreateCommand": "flutter pub get"
}
